{
  "name": "auto-updater",
  "version": "1.0.0",
  "description": "Auto updater for website data",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "collect": "ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ai",
    "tools",
    "mcp",
    "cursor",
    "updater"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@octokit/rest": "^19.0.0",
    "@types/cron": "^2.4.3",
    "axios": "^1.6.0",
    "cheerio": "1.0.0-rc.12",
    "cron": "^4.1.0",
    "dotenv": "^16.4.7",
    "node-fetch": "^2.7.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
